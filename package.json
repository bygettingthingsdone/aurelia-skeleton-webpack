    "aurelia-bootstrapper": "^1.0.0-beta.1.0.2",
    "aurelia-event-aggregator": "^1.0.0-beta.1",
    "aurelia-fetch-client": "^1.0.0-beta.1.0.2",
    "aurelia-framework": "^1.0.0-beta.1.0.8",
    "aurelia-logging-console": "^1.0.0-beta.1",
    "aurelia-templating-binding": "^1.0.0-beta.1.0.2",
    "aurelia-templating-resources": "^1.0.0-beta.1.0.4",
    "aurelia-templating-router": "^1.0.0-beta.1.0.5",
    "bootstrap": "^3.3.6",
    "font-awesome": "^4.5.0",
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
