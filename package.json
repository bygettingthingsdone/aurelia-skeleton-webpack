{
  "name": "aurelia-webpack",
  "version": "0.1.0",
  "description": "An attempt to get a working Aurelia skeleton app with webpack and NPM instead of SystemJS and jspm.",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.js --hot --progress --colors --devtool eval",
    "build": "webpack --config webpack.config.js --progress --profile --colors",
    "prod": "webpack -p --config webpack.prod.config.js --progress --colors",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Martijn Boland",
  "license": "MIT",
  "dependencies": {
