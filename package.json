    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^2.7.1",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "package": "^1.0.1",
